{
  "name": "zoom-to-elements-sample",
  "description": "Zoom To Elements Sample",
  "license": "MIT",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "buildIModelJsModule",
    "start": "node ./node_modules/@bentley/imodeljs-webserver/lib/WebServer.js --port=3000 --resources=./lib/webresources/",
    "lint": "tslint --project . 1>&2",
    "test": "",
    "clean": "rimraf lib .rush"
  },
  "iModelJs": {
    "buildModule": {
      "type": "application",
      "sourceResources": [
        {
          "source": "./src/**/*.scss",
          "dest": "./lib"
        },
        {
          "source": "./src/**/*.css",
          "dest": "./lib"
        },
        {
          "source": "./public/**/*",
          "dest": "./lib/webresources"
        },
        {
          "source": "./node_modules/@bentley/frontend-sample-base/public/**/*",
          "dest": "./lib/webresources"
        }
      ],
      "webpack": {
        "dest": "./lib/webresources",
        "entry": "./lib/index.js",
        "bundleName": "main",
        "styleSheets": true,
        "htmlTemplate": "./src/index.html"
      },
      "makeConfig": {
        "dest": "./lib/webresources/config.json",
        "sources": [
          {
            "file": "process.env",
            "filter": "^(i|I)(m|M)(j|J)(s|S)_"
          },
          {
            "file": "./config.json",
            "filter": "^(i|I)(m|M)(j|J)(s|S)_"
          },
          {
            "file": "./node_modules/@bentley/frontend-sample-base/config.json",
            "filter": "^(i|I)(m|M)(j|J)(s|S)_"
          }
        ]
      }
    }
  },
  "dependencies": {
    "@bentley/bentleyjs-core": "1.9.0",
    "@bentley/config-loader": "1.9.0",
    "@bentley/geometry-core": "1.9.0",
    "@bentley/icons-generic-webfont": "^0.0.22",
    "@bentley/imodeljs-clients": "1.9.0",
    "@bentley/imodeljs-common": "1.9.0",
    "@bentley/imodeljs-frontend": "1.9.0",
    "@bentley/imodeljs-i18n": "1.9.0",
    "@bentley/imodeljs-quantity": "1.9.0",
    "@bentley/imodeljs-webserver": "1.9.0",
    "@bentley/ui-components": "1.9.0",
    "@bentley/ui-abstract": "1.9.0",
    "@bentley/ui-core": "1.9.0",
    "@bentley/presentation-components": "1.9.0",
    "@bentley/presentation-frontend": "1.9.0",
    "@bentley/presentation-common": "1.9.0",
    "@bentley/frontend-sample-base": "1.9.0",
    "express": "^4.16.3",
    "inspire-tree": "^5.0.1",
    "lodash": "^4.17.10",
    "react": "^16.8.0",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.1",
    "react-dom": "^16.8.0",
    "react-redux": "^5.1.0",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@bentley/build-tools": "1.9.0",
    "@bentley/webpack-tools": "1.9.0",
    "@types/express": "^4.16.0",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "ignore-styles": "^5.0.1",
    "tslint": "^5.16.0",
    "typescript": "^3.5.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.0",
    "xmlhttprequest": "^1.8.0",
    "rimraf": "^2.6.2"
  },
  "homepage": "http://localhost:3000/",
  "proxy": "http://localhost:5000"
}
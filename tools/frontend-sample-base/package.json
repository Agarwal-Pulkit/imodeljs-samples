{
  "name": "@bentley/frontend-sample-base",
  "description": "Frontend Sample Base",
  "main": "lib/frontend-sample-base.js",
  "typings": "lib/frontend-sample-base",
  "license": "MIT",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "os": [
    "win32",
    "linux"
  ],
  "version": "1.8.0",
  "private": true,
  "scripts": {
    "build": "buildIModelJsModule",
    "lint": "tslint --project . 1>&2",
    "test": "",
    "clean": "rimraf lib .rush"
  },
  "iModelJs": {
    "buildModule": {
      "type": "system",
      "tscOptions": "-build",      
      "sourceResources": [
        {
          "source": "./src/**/*.css",
          "dest": "./lib"
        },
        {
          "source": "./src/**/*.scss",
          "dest": "./lib"
        }
      ],
      "webpack": {
        "dest": "./lib/module",
        "entry": "./lib/frontend-sample-base.js",
        "bundleName": "frontend-sample-base",
        "styleSheets" : true
      }
    }
  },
  "dependencies": {
    "@bentley/bentleyjs-core": "1.8.0",
    "@bentley/config-loader": "1.8.0",
    "@bentley/geometry-core": "1.8.0",
    "@bentley/icons-generic-webfont": "^0.0.22",
    "@bentley/imodeljs-clients": "1.8.0",
    "@bentley/imodeljs-common": "1.8.0",
    "@bentley/imodeljs-frontend": "1.8.0",
    "@bentley/imodeljs-i18n": "1.8.0",
    "@bentley/imodeljs-quantity": "1.8.0",
    "@bentley/ui-components": "1.8.0",
    "@bentley/ui-core": "1.8.0",
    "body-parser": "^1.18.3",
    "chai-jest-snapshot": "^2.0.0",
    "express": "^4.16.3",
    "inspire-tree": "^5.0.1",
    "lodash": "^4.17.10",
    "react": "^16.8.0",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.1",
    "react-dom": "^16.8.0",
    "tooltip.js": "^1.2.0"    
  },
  "devDependencies": {
    "@bentley/build-tools": "1.8.0",
    "@bentley/webpack-tools": "1.8.0",
    "@types/body-parser": "^1.17.0",
    "@types/bunyan": "^1.8",
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.0",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "glob": "^7.1.4",
    "ignore-styles": "^5.0.1",
    "jsdom": "^13.0.0",
    "jsdom-global": "^3.0.2",
    "nodemon": "^1.18.4",
    "tslint": "^5.16.0",
    "typescript": "^3.5.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.0",
    "xmlhttprequest": "^1.8.0",
    "rimraf": "^2.6.2",
    "popper.js": "^1.14.4"
  }
}
